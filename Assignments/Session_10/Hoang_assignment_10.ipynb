    "def show_stars(rows) :\n",
    "    star = 0\n",
    "    i = 0\n",
    "    sign = \"*\"\n",
    "    while (star < rows) :\n",
    "        while (i < star) :\n",
    "            sign+='*'\n",
    "            i+=1\n",
    "        print(sign)\n",
    "        star+=1"
   ]
